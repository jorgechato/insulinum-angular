a
    color: igrey;
    text-decoration: none;
    cursor: pointer;
    font-weight 600
    &:hover
        color maincolour
    &:active
        color maincolour

body
    font-family 'Open Sans', "Helvetica Neue",Helvetica,Arial,sans-serif
    font-size 16px
    font-weight: 400;
    color: igrey;

::selection {
    color: iwhite;
    background: maincolour;
}
body:parent { background: none; }
